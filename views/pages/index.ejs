<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('./layout/head.ejs')%>
</head>
<body>
    <header>
        <%- include('./layout/header.ejs')%>
    </header>
    <main class="contOFcont">
            <% holiday.forEach((holiday,index)=>{%>
        <section class="container-marah">
            <p class="name-marah"><%=holiday.name%></p>
            <img class="img-marah" src="<%=image[index].img%>" alt="">
            <p class="date-marah"><%=holiday.date%></p>
            <p class="type-marah"><%=holiday.type%></p>
            <p class="desc-marah"><%=holiday.description%></p>
        </section>
            <%})%>
    </main>
    <footer>
        <%- include('./layout/footer.ejs')%>
    </footer>
</body>
</html>
